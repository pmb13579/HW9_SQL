departments
-
  dept_no varchar(10) pk
  dept_name varchar(50)
  
employees
-
emp_no integer pk
birth_date date
first_name varchar(50)
last_name varchar(50)
gender varchar(20)
hire_date date

salaries
-
emp_no integer
salary integer
from_date date
to_date date

dept_emp
-
emp_no integer
dept_no varchar(10)
from_date date
to_date date

dept_manager
-
dept_no varchar(10)
emp_no integer
from_date date
to_date date

titles
-
emp_no integer
title varchar(50)
from_date date
to_date date
